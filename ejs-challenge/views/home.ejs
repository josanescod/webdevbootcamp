<%- include ("./partials/header") -%>
    <h1>Home</h1>
    <p>
        <%= homeContent %>
    </p>

    <% posts.forEach(post => { %>
        <div>
            <div class="container post">
                <h3>
                    <%= post.title %>
                </h3>
                <p>
                    <%= post.content %>
                </p>
            </div>
        </div>
        <% }); %>
<%- include ("./partials/footer") -%>